@model IEnumerable<HostelApp.Models.Employee>

<p>
    @Html.ActionLink("Добавить студента", "Add_Student")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>        
        <th>
            @Html.DisplayNameFor(model => model.LivingRoom.Num)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LivingRoom.Num)
        </td>
        <td>
            @Html.ActionLink("Подробнее", "Employee_Details", new { id=item.Id }) 
        </td>

    </tr>
}

</table>
